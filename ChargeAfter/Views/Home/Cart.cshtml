            <script type="text/template" class="accountNotifications">

                {{ if (data.length){ }}

                <div class="alert-list-item fs20 alerts-header">{{=count}} Notifications</div>
                <p class='remove-all-alerts'>Clear all</p>
                <div class="{{ if (count > 3){ }}scroller{{ } }}">
                    {{ _.each( data, function( itemData ){ }}
                    {{ if(itemData.type == 'priceDrop'){ }}
                    <div class="alert-type-wrapper">
                        <p class='alert-type-header'>Price Drop </p>
                        {{ _.each( itemData.data, function( itemD ){ }}
                        <div class="alert-list-item price-drop-alert fs14">
                            <a href="{{ if(itemD.itemType == 'CART'){ }}{{=BH.constants.findUrl}}cart.jsp{{ }else{ }}{{=itemD.detailsUrl}}{{ } }}">
                                <svg class="cart-icon price-drop-bell"><use xlink:href="#bell"></use></svg>
                                <span class="alert-text">{{=itemD.brand}} {{=itemD.shortDescription}} just got <span class='bold'>${{=itemD.dropAmount}} cheaper</span>. act now!</span>
                                <span data-removeurl='https://www.bhphotovideo.com/bnh/controller/home?Q=&O=&A=DeleteNotification&key={{=itemD.key}}' class="remove-alert"><svg><use xlink:href="#x"></use></svg></span>
                                <div class="clearfix"></div>
                            </a>
                        </div>
                        {{ }); }}
                    </div>
                    {{ }else if(itemData.type == 'rebateExp'){ }}
                    <div class="alert-type-wrapper">
                        <p class='alert-type-header'>Rebates </p>
                        {{ _.each( itemData.data, function( itemD ){ }}
                        <div class="alert-list-item price-drop-alert fs14">
                            <a href="{{ if(itemD.itemType == 'CART'){ }}{{=BH.constants.findUrl}}cart.jsp{{ }else{ }}{{=itemD.detailsUrl}}{{ } }}">
                                <svg class="cart-icon price-drop-bell"><use xlink:href="#bell"></use></svg>
                                <span class="alert-text">
                                    Rebates on {{=itemD.brand}} {{=itemD.shortDescription}} will expire
                                    <span class='bold'>
                                        {{ if(itemD.expiringInXdays == 1){ }}in {{=itemD.expiringInXdays}} day{{ }else if(itemD.expiringInXdays == 0){ }} today {{ }else{ }} in {{=itemD.expiringInXdays}} days {{ } }}
                                    </span>
                                </span>
                                <span data-removeurl='https://www.bhphotovideo.com/bnh/controller/home?Q=&O=&A=DeleteNotification&key={{=itemD.key}}' class="remove-alert"><svg><use xlink:href="#x"></use></svg></span>
                                <div class="clearfix"></div>
                            </a>
                        </div>
                        {{ }); }}
                    </div>
                    {{ }else if(itemData.type == 'RECOMMENDED_ACCESSORIES'){ }}
                    <div class="alert-type-wrapper">
                        <p class='alert-type-header'>Accessories </p>
                        {{ _.each( itemData.data, function( itemD ){ }}
                        <div class="alert-list-item fs14">
                            <a href="{{=itemD.link}}">
                                {{ if (itemData.type == "CART"){ }}
                                <svg class="cart-icon"><use xlink:href="#cart"></use></svg>
                                {{ } else { }}
                                <svg class="cart-icon"><use xlink:href="#bell"></use></svg>
                                {{ } }}

                                <span class="alert-text">{{=itemD.text}}</span>
                                <span data-removeurl='https://www.bhphotovideo.com/bnh/controller/home?Q=&O=&A=DeleteNotification&key={{=itemD.key}}' class="remove-alert"><svg><use xlink:href="#x"></use></svg></span>
                                <div class="clearfix"></div>
