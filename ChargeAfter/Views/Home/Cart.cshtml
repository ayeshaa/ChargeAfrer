                                            <script type="text/ng-template" id="pickupLayer.html">
                                                <div class="pickup-layer">
                                                    <div class="break"></div>
                                                    <div class="clearfix">
                                                        <ui-gmap-google-map ng-if="vm.showMap"
                                                                            class="left pickup-locations-map"
                                                                            center="vm.map.center"
                                                                            zoom="vm.map.zoom"
                                                                            pan="false">
                                                            <ui-gmap-markers models="vm.map.markers"
                                                                             coords="'self'"
                                                                             events="vm.map.markerEvents"
                                                                             options="'options'"></ui-gmap-markers>
                                                        </ui-gmap-google-map>
                                                        <div class="right pickup-options">
                                                            <h3 class="header c41">Select a Pickup Location</h3>
                                                            <div ng-if="vm.allLocationsClosed()">
                                                                <p class="semibold">Unfortunately, both of our locations are closed. Your pickup will be ready 45 minutes after opening</p>
                                                                <div class="break"></div>
                                                                <div class="break"></div>
                                                            </div>
                                                            <div ng-if="vm.isAnyLocationClosingSoon()">
                                                                <p class="semibold">We're sorry: our pickup location(s) are about to close. Your order may not be ready for pickup today. We apologize for any inconvenience.</p>
                                                                <div class="break"></div>
                                                                <div class="break"></div>
                                                            </div>
                                                            <div ng-repeat="pickupLocation in vm.pickupLocations | orderBy: 'id'">
                                                                <div class="pickup-location"
                                                                     ng-class="{'selected': pickupLocation.selected, 'highlight': vm.isHoveringMarker(pickupLocation.name)}"
                                                                     ng-mouseenter="vm.startMarkerBounce(pickupLocation.name)"
                                                                     ng-mouseleave="vm.endMarkerBounce(pickupLocation.name)">
                                                                    <div class="pickup-location-info fs14" ng-class="{'c35': !vm.isLocationPickupAvailable(pickupLocation.name)}">
                                                                        <p class="pickup-location-title fs20">{{pickupLocation.title}}</p>
                                                                        <p class="pickup-location-address">{{pickupLocation.address.addressLine1}} {{pickupLocation.getCityStateZip()}}</p>
                                                                        <p>
                                                                            <span class="bold">{{pickupLocation.isNowOpen || pickupLocation.isOpeningLaterToday ? "Open Today:" : "Closed"}}</span>
                                                                            <span ng-if="pickupLocation.isNowOpen || pickupLocation.isOpeningLaterToday">{{pickupLocation.todaysHours}}</span>
                                                                            |
                                                                            <span class="bold">
                                                                                {{pickupLocation.isOpenTomorrow ? (pickupLocation.isNowOpen || pickupLocation.isOpeningLaterToday ? "" : "Open ") + "Tomorrow:" : "Next Open: " + pickupLocation.nextOpenDaysDate}}
                                                                            </span>
                                                                            {{pickupLocation.nextOpenDaysHours}}
                                                                        </p>
                                                                        <bh-time-based end-date="1483315200000" ng-if="pickupLocation.name === 'store'">
                                                                            <p class="gift-wrap-available fs14" style="padding: 3px 0 0 2px;">
                                                                                <img style="margin-right: 2px; margin-bottom: -1px;" alt="Free gift warap available" src="data:image/png;base64," />
                                                                                Free gift wrap available at NYC Superstore
                                                                            </p>
                                                                        </bh-time-based>
                                                                        <p ng-if="!pickupLocation.isNowOpen && !vm.allLocationsClosed()">Pickup will be ready 45 minutes after opening.</p>
                                                                    </div>
                                                                    <div ng-if="vm.isLocationPickupAvailable(pickupLocation.name)" class="full-width bold block center-align btn-select select-pickup-location" ng-click="vm.setPickupLocation(pickupLocation.name)">
                                                                        {{pickupLocation.selected ? "Selected" : "Select"}}
                                                                        <bh-icon ng-show="pickupLocation.selected" bh-icon-style="13px 14px #fff">check</bh-icon>
                                                                    </div>
                                                                    <div ng-if="!vm.isLocationPickupAvailable(pickupLocation.name)" class="pickup-not-available c7">
                                                                        <bh-icon class="inlineBlock" bh-icon-style="18px 18px #990000">error-light</bh-icon>
                                                                        <span class="inlineBlock c7">Pickup Not Available</span>

                                                                    </div>
                                                                </div>
                                                                <div class="break"></div>
                                                                <div class="break"></div>
                                                            </div>
                                                            <p>Orders not picked up within 5 business days will be canceled.</p>
                                                        </div>
                                                    </div>
                                                    <div class="break"></div>
                                                </div>
                                            </script>



