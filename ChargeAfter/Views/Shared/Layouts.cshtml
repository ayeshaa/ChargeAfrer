                <script>
	(function(document){
		var shippingWrapper = document.getElementsByClassName("shippingZipWrapper")[0];
		var freeShippingMap = document.getElementById("freeShippingMap");
		var shipDest=function(){
			if(shippingWrapper.className.indexOf("changeZip")!==-1){
				shippingWrapper.classList.remove("changeZip");
			}else{
				shippingWrapper.classList.add("changeZip");
				document.getElementsByClassName("shippingZipInput")[0].focus();
			}
		}
		var shippingZipButton=function(){
			var $error = $(".shippingZipError");
			$error.addClass("displayNone");
			var inputZip = $(".shippingZipInput").val();
			var isZip = /^\d{5}$/.test(inputZip);
			if(!isZip){
				$error.removeClass("displayNone");
				return false;
			}
			$(document).trigger("updateZipDetail",{zip:inputZip});
			$(".shippingZipInput").val("");
			getShippingData(inputZip);

		}
		var showShipMap = function(){
			if(freeShippingMap.className.indexOf("showMap")!==-1){
				freeShippingMap.classList.remove("showMap");
			}else{
				freeShippingMap.classList.add("showMap");
			}

			$('#extendedReturnDetail, .extendedReturnDetailWrapper').hide();

		}

		var ShipTimer = {
			init: function($el){
				if (this.activeShipTimer) {
					this.activeShipTimer.destroy();
				}

				var deliveredBy = $el.data('deliverby');
				var cutoffDataArray = deliveredBy.data;
				var cutoffHour = deliveredBy.cutoffHour;
				var cutoffTimes = [deliveredBy.cutoffTime, deliveredBy.nextCutOfftime];
				var setTimer = function(){
					if(cutoffDataArray) {
						/*
						var cutoffData = cutoffDataArray[0];
						// var cutoffHour = 2;//cutoffData.cutoffHour;
						$el.find('.js-deliveryDate').html(cutoffData.preferredMethod.deliveryDate);
						$el.find('.js-preffered-name').html(cutoffData.preferredMethod.name);
						if(cutoffData.fasterMethod) {
							$el.find('.ship-sooner-date').html(cutoffData.fasterMethod.deliveryDate);
						}
						*/
					}
					this.activeShipTimer = BH.globals.addTimeCountdown({
						endTime: (cutoffTimes[0] - 1000),
						onTick: function(timeObject){
							// console.log(timeObject);
							ShipTimer.update( timeObject, cutoffTimes, $el, cutoffHour );
						},
						onEnd: function(){
							cutoffTimes.shift();
							if(cutoffDataArray){
								cutoffDataArray.shift();
							}
							setTimer();
						}
					});

				};
				setTimer();
			},
			activeShipTimer: undefined,
			update: function(timeObject, cutoffs, $element, cutoffHour){
				// console.log("UPDATE : ",timeObject,cutoffs,$element,cutoffHour);
				if(!cutoffs.length) return;
				var shipDay = '',
				    twoHours = 7200000,
					showCountdown = false,
					now = timeObject.currentTime,
					daysDiff = ShipTimer.daysDiff(cutoffs[0], now);
			        if (daysDiff === 0){
					shipDay = "today";//translate('today');
					if(cutoffs[0]-now < twoHours)
						showCountdown = true
					}else if (daysDiff >= 1){
					$element.find(".shipTimerText").addClass("displayNone");
					$element.find('.shipDay').addClass("displayNone");
			        }
				// console.log("daysDiff",daysDiff,"shipday : ",shipDay,"showCountdown",showCountdown,"cutoffHour:",cutoffHour);
				if(showCountdown) {
					var rv = ShipTimer.print(timeObject.millisRemaining);
					$element.find('.shipTimerText').html("Order in the next ");
					// console.log(rv.timer);
					$element.find('.shipTimer').html(rv.timer);
					// if(!cutoffHour)
						// $element.find('.shipDay').text(shipDay);
				}else {
					if(cutoffHour){
						// $element.find('.shipDay').html(shipDay);
						$element.find('.shipTimerText').html("Order by "+cutoffHour+" ET");
						$element.find(".shipTimer").addClass("displayNone");
					}
				}

			},

			print: function(milisecsTo){

				var rv = {},
					secs = parseInt((milisecsTo / 1000)),
					mins = parseInt((secs / 60)),
					hours = parseInt(mins / 60);

				rv.timer = (hours > 0 ? hours + ":" : "");
				rv.hoursOrMinutes = hours > 0 ? "hours" : "minutes";

				rv.timer += ((mins % 60 + "").length == 2 ?  mins % 60 : "0" + mins % 60) + ":";
				rv.timer += ((secs % 60 + "").length == 2 ?  secs % 60 : "0" + secs % 60);

				return rv;
			},

			daysDiff : function(laterDateMillis, earlierDateMillis){

				var earlierDate = new Date(earlierDateMillis).setHours(0, 0, 0, 0),
					laterDate = new Date(laterDateMillis).setHours(0, 0, 0, 0);

				return Math.round( (laterDate - earlierDate) / 86400000 );
			}

		}
		var updateTransit=function(transitTime,hasZip,hasShippingTomorrow,city){
			var $el = $(".freeShippingContainer");
			switch(transitTime){
				case 1 :
					$el.find(".shipTransitTime").html("Free Next Day Shipping* to ");

					// console.log("hasShippingTomorrow",hasShippingTomorrow);
					$el.find(".shipTimer").removeClass("displayNone");
					$el.find(".shipTimerText").removeClass("displayNone");
					if(hasShippingTomorrow){

						$el.find('.shipDay').removeClass("displayNone").text(", get it Tomorrow");
					}
					// }else{
						// $el.find('.shipDay').removeClass("displayNone").text("");
					// }
					$(".freeShippingMapShipText").html("Free Next Day Shipping (1 business day) to "+city);
				break;
				case 2 :
					$el.find(".shipTimerText").removeClass("displayNone");
					$el.find(".shipTransitTime").html("Free 2-Day Shipping* to ");
					$el.find('.shipDay').addClass("displayNone");
					$el.find(".shipTimer").removeClass("displayNone");
					$(".freeShippingMapShipText").html("Free 2-Day Shipping (2 business days) to "+city);
				break;
				default :
					$el.find(".shipTransitTime").html("Free Expedited Shipping* to ");
					$el.find('.shipDay').addClass("displayNone");
					$el.find(".shipTimerText").addClass("displayNone");
					$el.find(".shipTimer").addClass("displayNone");
					$(".freeShippingMapShipText").html("Free Expedited Shipping (3 business days) to "+city);
				break;
			}
		}

		var getShippingData=function(zipCode){
			var zip=zipCode||"";
			var url = BH.constants.findUrl + "json/getDeliveredBy.jsp&zip="+zip;
			$.get(url).done(function(data){
				if(data.status=="error"){
					if(data.isApoState){
						$(".shippingZipError").html("Not available for this ZIP code").removeClass("displayNone");
					}else{
						$(".shippingZipError").html("Unrecognized Zip Code").removeClass("displayNone");
					}
				}else{
					if(shippingWrapper.className.indexOf("changeZip")!==-1){
						document.getElementsByClassName("shippingZipBG")[0].click();
					}
					var $el = $(".freeShippingContainer");
					$el.data("deliverby",data);
					if(data.city){
						$el.find(".shipDest").removeClass("needZipChange").html(data.city.substring(0,data.city.length-4));
					}else{
						$el.find(".shipDest").html("change ZipCode").addClass("needZipChange");
					}
					updateTransit(data.transitTime,(zipCode?true:false),data.hasShippingTomorrow,(data.city ? data.city.substring(0,data.city.length-4) : ""));
					ShipTimer.init($el);
					$("#freeShippingWrapper").removeClass("displayNone");
				}
			});
		}
		document.getElementsByClassName("shipDest")[0].addEventListener("click", shipDest,false);
		document.getElementsByClassName("shippingZipBG")[0].addEventListener("click", shipDest,false);
		document.getElementsByClassName("shippingZipButton")[0].addEventListener("click", shippingZipButton,false);
		document.getElementsByClassName("shipDetails")[0].addEventListener("click", showShipMap,false);
		document.getElementsByClassName("freeShippingCloseBtn")[0].addEventListener("click", showShipMap,false);
		var waitForJqueryReady = function(){
			if(window.jQuery && BH && BH.globals && BH.constants){
				$(document).on("updateZipBanner",function(e,data){
					getShippingData(data.zip);
				});
				getShippingData();
				$(".shippingZipInput").keydown(function (e) {
					// console.log(e.keyCode);
					if($(this).val().length>4 && e.keyCode==13){//respond to enter
						document.getElementsByClassName("shippingZipButton")[0].click();
						e.preventDefault();
						return;
					}
					if($(this).val().length>4 && e.keyCode!=8 && e.keyCode!=46){ //make sure you can backspace
						$(this).val("");
						return;
					}
					if ($.inArray(e.keyCode, [46, 8, 9, 27, 13, 110]) !== -1 || (e.keyCode == 65 && e.ctrlKey === true) || (e.keyCode == 67 && e.ctrlKey === true) || (e.keyCode == 88 && e.ctrlKey === true) || (e.keyCode >= 35 && e.keyCode <= 39)) {
						return;
					}
					if ((e.shiftKey || (e.keyCode < 48 || e.keyCode > 57)) && (e.keyCode < 96 || e.keyCode > 105)) {
						e.preventDefault();
					}
				});
			}else{
				setTimeout(function(){waitForJqueryReady()},100);
			}
		}
		waitForJqueryReady();

		lazyLoad.lazyRunJs(function(){
			$('.returnDetail, .returnDetailClose').on('click', function(){
				if(freeShippingMap.className.indexOf("showMap")!==-1){
					freeShippingMap.classList.remove("showMap");
				}
				$('#extendedReturnDetail, .extendedReturnDetailWrapper').toggle();
			});
		});

	})(document);
                </script>




